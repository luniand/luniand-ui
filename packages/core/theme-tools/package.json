{
  "name": "@luniand-ui/theme-tools",
  "version": "0.0.1",
  "description": "Luniand UI | Theme Tools module",
  "author": "Albert Tran trantoan.fox.97@gmail.com",
  "keywords": [
    "core",
    "styled-system",
    "luniand"
  ],
  "homepage": "https://github.com/luniand/luniand-ui#readme",
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
