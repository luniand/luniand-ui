  "description": "Luniand UI | System module",
  "author": "Albert Tran trantoan.fox.97@gmail.com",
  "keywords": [
    "core",
    "styled-system",
    "luniand"
  ],
  "homepage": "https://github.com/luniand/luniand-ui#readme",
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luniand/luniand-ui.git",
