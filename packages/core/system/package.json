    "@luniand-ui/styled-system": "workspace:*",
    "@luniand-ui/utils": "workspace:*",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.memoize": "^4.1.2"
  },
  "devDependencies": {
    "@types/lodash.camelcase": "^4.3.7",
