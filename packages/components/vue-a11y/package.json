    "build:fast": "tsup src/index.ts --format=esm,cjs",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "vue": ">=3"
  },
  "devDependencies": {
    "clean-package": "2.1.1",
    "vue": ">=3"
